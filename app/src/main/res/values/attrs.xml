<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FoodCardView">
        <attr name="cardTitle" format="string"/>
        <attr name="cardColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="CircularProgressBar">
        <attr name="seekPercentage" format="string"/>
        <attr name="seekBarColor" format="color"/>
        <attr name="innerCircleColor" format="color"/>
        <attr name="seekBarTextColor" format="color"/>
        <attr name="seekBarType">
            <enum name="percentage" value="0"/>
            <enum name="kcal" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="CustomDateView">
        <attr name="date" format="integer"/>
        <attr name="day_c" format="string"/>
        <attr name="month_c" format="string"/>
        <attr name="year_c" format="string"/>
        <attr name="weekStart_c" format="string"/>
        <attr name="weekEnd_c" format="string"/>
        <attr name="weekStartMonth_c" format="string"/>
        <attr name="weekEndMonth_c" format="string"/>
        <attr name="type_c" format="integer"/>
    </declare-styleable>
    <declare-styleable name="PieChart">
        <attr name="pieData" format="string"/>
    </declare-styleable>
</resources>